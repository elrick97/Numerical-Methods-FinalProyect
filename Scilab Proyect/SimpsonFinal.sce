    n = strtod(handles.iN.string)
    deff("dY = F(x)", "dY="+(handles.ecuacion.string))
    
    dH = (dB - dA) / n
    dSuma = 0
    for i = 1 : 2 : n - 1 
        dSuma = dSuma +  4*(F(dA + i*dH))
    end
     for i = 2 : 2 : n - 2
